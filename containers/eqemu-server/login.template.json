{
    "database": {
      "host": "db",
      "port": "3306",
      "db": "${DATABASE_NAME}",
      "user": "${DATABASE_USER}",
      "password": "${DATABASE_PASSWORD}",
      "subsystem": "MySQL"
    },
    "account": {
      "auto_create_accounts": true,
      "auto_account_activate": true
    },
    "worldservers": {
      "unregistered_allowed": true,
      "show_player_count": false,
      "dev_test_servers_list_bottom": false,
      "special_character_start_list_bottom": false,
      "reject_duplicate_servers": false
    },
    "security": {
      "plugin": "EQEmuAuthCrypto",
      "mode": 5,
      "allow_password_login": true,
      "allow_token_login": true
    },
    "logging": {
      "trace": false,
      "world_trace": false,
      "dump_packets_in": false,
      "dump_packets_out": false,
      "failed_login_log": false,
      "good_loginIP_log": false
    },
    "client_configuration": {
      "listen_port": 5998,
      "local_network": "${SERVER_ADDRESS}",
      "network_ip": "${SERVER_ADDRESS}",
      "salt": "randomstring"
    },
    "Old": {
      "port": 6000,
      "opcodes": "login_opcodes_oldver.conf"
    },
    "schema": {
      "access_log_table": "tblaccountaccesslog",
      "account_table": "tblLoginServerAccounts",
      "world_registration_table": "tblWorldServerRegistration",
      "world_admin_registration_table": "tblServerAdminRegistration",
      "world_server_type_table": "tblServerListType",
      "loginserver_setting_table": "tblloginserversettings"
    }
  }
